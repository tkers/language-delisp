'.source.delisp':
  'define':
    'prefix': 'def'
    'body': '(define ${1:symbol} ${2:value})'
  'define lambda':
    'prefix': 'fn'
    'body': """
      (define ${1:symbol}
        (lambda (${2:params})
          ${3:body}))
    """
  'if':
    'prefix': 'if'
    'body': """
      (if ${1:test}
        ${2:then}
        ${3:else})
    """
